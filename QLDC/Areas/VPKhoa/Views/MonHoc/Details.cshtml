@using QLDC.Models;
@model QLDC.Models.MonHoc

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/VPKhoa/Views/Shared/_Layout.cshtml";

}

<div class="container-fluid">
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h4 class="m-0 font-weight-bold text-primary">Thông tin môn học</h4>
        </div>
        <div class="card-body">
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.MaMon)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.MaMon)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TenMon)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TenMon)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.SoTinChi)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.SoTinChi)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.HinhThucThi)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.HinhThucThi)
                </dd>
            </dl>
            <div class="form-group">
                <div class="col-md-offset-2 ">
                    @Html.ActionLink("Quay lại", "Index", new {}, new { @class = "btn btn-danger" })
                </div>
            </div>
        </div>
    </div>
</div>

